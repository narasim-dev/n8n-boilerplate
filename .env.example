# Network
# NETWORK_NAME=docker_default

# Docker user (run: id -u and id -g to get your values)
UID=1000
GID=1000

# n8n Configuration
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http

# Encryption Key (generate: openssl rand -hex 16)
N8N_ENCRYPTION_KEY=

# Webhook URL
# N8N_WEBHOOK_URL=http://localhost:5678/

# Database
DB_TYPE=sqlite

# Timezone
TZ=America/Los_Angeles

# Logging
N8N_LOG_LEVEL=info
N8N_METRICS=false


# Activate workflows after import
# "true" or "all" - activate all workflows
# "Workflow1,Workflow2" - activate only listed (comma-separated names)
N8N_BOOTSTRAP_ACTIVATE_WORKFLOWS=true

# Code Execution Nodes (security risk!)
# In n8n v2, Code/Execute Command/SSH nodes are disabled by default.
# To enable them, uncomment:
# NODES_EXCLUDE=[]
