# n8n Configuration
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http

# Basic Authentication (change for production)
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=should-change-before-deploy

# Encryption Key (generate a new 32-character key for production)
N8N_ENCRYPTION_KEY=your-32-char-encryption-key-here

# Webhook URL
N8N_WEBHOOK_URL=http://localhost:5678/

# Database (SQLite, stored in /home/node/.n8n/database.sqlite)
DB_TYPE=sqlite

# Timezone
TZ=America/Los_Angeles

# Logging
N8N_LOG_LEVEL=info
N8N_METRICS=false

# User Management (disabled for simplicity)
N8N_USER_MANAGEMENT_DISABLED=true
